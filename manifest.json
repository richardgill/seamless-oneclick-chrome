
{
  "manifest_version": 2,

  "name": "Seamless one click buy",
  "description": "Buy your seamless orders in one click",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["http://www.seamlessuk.co.uk/*", "https://www.seamlessuk.co.uk/*"],
      "js": ["lib/jquery.min.js", "injector.js"]
    }
  ],
  // "browser_action": {
  //   "default_icon": "icon.png",
  //   "default_popup": "index.html"
  // },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage"
  ]
}
